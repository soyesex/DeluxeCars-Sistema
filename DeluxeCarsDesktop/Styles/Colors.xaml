<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="Color.Brand.NavyDark">#061229</Color>
    <Color x:Key="Color.Brand.NavyDeep">#0D2149</Color>
    <Color x:Key="Color.Brand.CharcoalDark">#0A192F</Color>
    <Color x:Key="Color.Brand.CharcoalMedium">#1C2630</Color>
    <Color x:Key="Color.Brand.CharcoalLight">#1F2A44</Color>
    <Color x:Key="Color.Brand.SlateGray">#2D3748</Color>
    <Color x:Key="Color.Brand.GraySteel">#4D5B7C</Color>
    <Color x:Key="Color.Brand.GrayMedium">#404040</Color>
    <Color x:Key="Color.Brand.GrayLight">#808080</Color>

    <Color x:Key="Color.Neutral.WhitePure">#FFFFFF</Color>
    <Color x:Key="Color.Neutral.WhitePrimary">#E0E6F1</Color>
    <Color x:Key="Color.Neutral.WhiteSecondary">#E0E0E0</Color>
    <Color x:Key="Color.Neutral.WhiteMuted">#C8C8C8</Color>
    <Color x:Key="Color.Neutral.WhiteSilver">#A9A9A9</Color>

    <Color x:Key="Color.Accent.ElectricBlue">#28AEED</Color>
    <Color x:Key="Color.Accent.DeepSkyBlue">#00BFFF</Color>
    <Color x:Key="Color.Accent.ActionPurple">#512bd4</Color>
    <Color x:Key="Color.Accent.ActionPurpleHover">#6A44E6</Color>
    <Color x:Key="Color.Accent.DestructiveRed">#D72638</Color>
    <Color x:Key="Color.Accent.ErrorRed">#D7596D</Color>

    <Color x:Key="Color.Menu.BlueSteel">#336699</Color>
    <Color x:Key="Color.Menu.BlueCornflower">#6699CC</Color>
    <Color x:Key="Color.Menu.BlueSky">#99CCFF</Color>
    <Color x:Key="Color.Menu.Lavender">#CCCCFF</Color>
    <Color x:Key="Color.Menu.OrangePastel">#FF9966</Color>
    <Color x:Key="Color.Menu.YellowGold">#FFCC66</Color>
    <Color x:Key="Color.Menu.Teal">#2A9D8F</Color>
    <Color x:Key="Color.Menu.GreenForest">#669966</Color>
    <Color x:Key="Color.Menu.Cyan">#66CCCC</Color>
    <Color x:Key="Color.Menu.PurpleRoyal">#9966CC</Color>
    <Color x:Key="Color.Menu.Magenta">#CC6699</Color>

    <SolidColorBrush x:Key="Brush.Brand.NavyDark" Color="{StaticResource Color.Brand.NavyDark}"/>
    <SolidColorBrush x:Key="Brush.Brand.NavyDeep" Color="{StaticResource Color.Brand.NavyDeep}"/>
    <SolidColorBrush x:Key="Brush.Brand.CharcoalDark" Color="{StaticResource Color.Brand.CharcoalDark}"/>
    <SolidColorBrush x:Key="Brush.Brand.CharcoalMedium" Color="{StaticResource Color.Brand.CharcoalMedium}"/>
    <SolidColorBrush x:Key="Brush.Brand.CharcoalLight" Color="{StaticResource Color.Brand.CharcoalLight}"/>
    <SolidColorBrush x:Key="Brush.Brand.SlateGray" Color="{StaticResource Color.Brand.SlateGray}"/>
    <SolidColorBrush x:Key="Brush.Brand.GraySteel" Color="{StaticResource Color.Brand.GraySteel}"/>
    <SolidColorBrush x:Key="Brush.Brand.GrayMedium" Color="{StaticResource Color.Brand.GrayMedium}"/>
    <SolidColorBrush x:Key="Brush.Brand.GrayLight" Color="{StaticResource Color.Brand.GrayLight}"/>

    <SolidColorBrush x:Key="Brush.Neutral.WhitePure" Color="{StaticResource Color.Neutral.WhitePure}"/>
    <SolidColorBrush x:Key="Brush.Neutral.WhitePrimary" Color="{StaticResource Color.Neutral.WhitePrimary}"/>
    <SolidColorBrush x:Key="Brush.Neutral.WhiteSecondary" Color="{StaticResource Color.Neutral.WhiteSecondary}"/>
