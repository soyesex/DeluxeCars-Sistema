    <SolidColorBrush x:Key="Brush.Neutral.WhiteSilver" Color="{StaticResource Color.Neutral.WhiteSilver}"/>

    <SolidColorBrush x:Key="Brush.Accent.ElectricBlue" Color="{StaticResource Color.Accent.ElectricBlue}"/>
    <SolidColorBrush x:Key="Brush.Accent.DeepSkyBlue" Color="{StaticResource Color.Accent.DeepSkyBlue}"/>
    <SolidColorBrush x:Key="Brush.Accent.ActionPurple" Color="{StaticResource Color.Accent.ActionPurple}"/>
    <SolidColorBrush x:Key="Brush.Accent.ActionPurpleHover" Color="{StaticResource Color.Accent.ActionPurpleHover}"/>
    <SolidColorBrush x:Key="Brush.Accent.DestructiveRed" Color="{StaticResource Color.Accent.DestructiveRed}"/>
    <SolidColorBrush x:Key="Brush.Accent.ErrorRed" Color="{StaticResource Color.Accent.ErrorRed}"/>

    <SolidColorBrush x:Key="Brush.Menu.BlueSteel" Color="{StaticResource Color.Menu.BlueSteel}"/>
    <SolidColorBrush x:Key="Brush.Menu.BlueCornflower" Color="{StaticResource Color.Menu.BlueCornflower}"/>
    <SolidColorBrush x:Key="Brush.Menu.BlueSky" Color="{StaticResource Color.Menu.BlueSky}"/>
    <SolidColorBrush x:Key="Brush.Menu.Lavender" Color="{StaticResource Color.Menu.Lavender}"/>
    <SolidColorBrush x:Key="Brush.Menu.OrangePastel" Color="{StaticResource Color.Menu.OrangePastel}"/>
    <SolidColorBrush x:Key="Brush.Menu.YellowGold" Color="{StaticResource Color.Menu.YellowGold}"/>
    <SolidColorBrush x:Key="Brush.Menu.Teal" Color="{StaticResource Color.Menu.Teal}"/>
    <SolidColorBrush x:Key="Brush.Menu.GreenForest" Color="{StaticResource Color.Menu.GreenForest}"/>
    <SolidColorBrush x:Key="Brush.Menu.Cyan" Color="{StaticResource Color.Menu.Cyan}"/>
    <SolidColorBrush x:Key="Brush.Menu.PurpleRoyal" Color="{StaticResource Color.Menu.PurpleRoyal}"/>
    <SolidColorBrush x:Key="Brush.Menu.Magenta" Color="{StaticResource Color.Menu.Magenta}"/>


    <SolidColorBrush x:Key="Background.Primary" Color="{StaticResource Color.Brand.CharcoalDark}"/>
    <SolidColorBrush x:Key="Background.Secondary" Color="{StaticResource Color.Brand.CharcoalMedium}"/>
    <SolidColorBrush x:Key="Background.Surface" Color="{StaticResource Color.Brand.CharcoalLight}"/>
    <SolidColorBrush x:Key="Background.Surface.Hover" Color="#2C3A5A"/>
    <SolidColorBrush x:Key="Background.Surface.Selected" Color="{StaticResource Color.Menu.BlueSteel}"/>
    <SolidColorBrush x:Key="Background.Disabled" Color="{StaticResource Color.Brand.SlateGray}"/>

    <SolidColorBrush x:Key="Text.Primary" Color="{StaticResource Color.Neutral.WhitePrimary}"/>
    <SolidColorBrush x:Key="Text.Secondary" Color="{StaticResource Color.Neutral.WhiteSilver}"/>
    <SolidColorBrush x:Key="Text.Placeholder" Color="#8A93A2"/>
    <SolidColorBrush x:Key="Text.Disabled" Color="#6A7383"/>
    <SolidColorBrush x:Key="Text.Action" Color="{StaticResource Color.Neutral.WhitePure}"/>
    <SolidColorBrush x:Key="Text.Error" Color="{StaticResource Color.Accent.ErrorRed}"/>

    <SolidColorBrush x:Key="Border.Default" Color="{StaticResource Color.Brand.GraySteel}"/>
    <SolidColorBrush x:Key="Border.Focus" Color="{StaticResource Color.Accent.ElectricBlue}"/>

    <SolidColorBrush x:Key="Action.Primary.Background" Color="{StaticResource Color.Accent.ActionPurple}"/>
    <SolidColorBrush x:Key="Action.Primary.Hover" Color="{StaticResource Color.Accent.ActionPurpleHover}"/>
    <SolidColorBrush x:Key="Action.Destructive.Background" Color="{StaticResource Color.Accent.DestructiveRed}"/>
    <SolidColorBrush x:Key="Action.Destructive.Foreground" Color="{StaticResource Color.Neutral.WhitePure}"/>

