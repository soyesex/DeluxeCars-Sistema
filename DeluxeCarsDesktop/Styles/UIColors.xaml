<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="Color.PrimaryBack1">#0A0E21</Color>
    <Color x:Key="Color.PrimaryBack2">#151933</Color>
    <Color x:Key="Color.SecundaryBack1">#1F2A44</Color>
    <Color x:Key="Color.SecundaryBack2">#273353</Color>
    <Color x:Key="Color.color11">#339999</Color>

    <Color x:Key="Color.WinBorder1">#336699</Color>
    <Color x:Key="Color.WinBorder2">#6699CC</Color>
    <Color x:Key="Color.WinBorder3">#004488</Color>

    <Color x:Key="Color.Title1">#E0E0E0</Color>
    <Color x:Key="Color.Title2">#C8C8C8</Color>
    <Color x:Key="Color.PlainText1">#808080</Color>

    <Color x:Key="Color.Panel">#191F28</Color>
    <Color x:Key="Color.PanelOver">#212831</Color>

    <Color x:Key="Color.Accent1">#336699</Color>
    <Color x:Key="Color.Accent2">#6699CC</Color>
    
    <SolidColorBrush x:Key="primaryBackColor1" Color="{StaticResource Color.PrimaryBack1}"/>
    <SolidColorBrush x:Key="primaryBackColor2" Color="{StaticResource Color.PrimaryBack2}"/>
    <SolidColorBrush x:Key="secundaryBackColor1" Color="{StaticResource Color.SecundaryBack1}"/>
    <SolidColorBrush x:Key="secundaryBackColor2" Color="{StaticResource Color.SecundaryBack2}"/>

    <SolidColorBrush x:Key="titleColor2" Color="{StaticResource Color.Title2}"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="{StaticResource Color.PlainText1}"/>

    <SolidColorBrush x:Key="winBorderColor1" Color="{StaticResource Color.WinBorder1}"/>
    <SolidColorBrush x:Key="winBorderColor2" Color="{StaticResource Color.WinBorder2}"/>
    <SolidColorBrush x:Key="winBorderColor3" Color="{StaticResource Color.WinBorder3}"/>

    <SolidColorBrush x:Key="color1" Color="#336699"/>
    <SolidColorBrush x:Key="color2" Color="#6699CC"/>
    <SolidColorBrush x:Key="color3" Color="#99CCFF"/>
    <SolidColorBrush x:Key="color4" Color="#CCCCFF"/>
    <SolidColorBrush x:Key="color5" Color="#FFCC66"/>
    <SolidColorBrush x:Key="color6" Color="#FF9966"/>
    <SolidColorBrush x:Key="color7" Color="#CC6699"/>
    <SolidColorBrush x:Key="color8" Color="#9966CC"/>
    <SolidColorBrush x:Key="color9" Color="#66CCCC"/>
    <SolidColorBrush x:Key="color10" Color="#669966"/>
    <SolidColorBrush x:Key="color12" Color="#CCCC66"/>
    <SolidColorBrush x:Key="color13" Color="#336633"/>
    
    
    <SolidColorBrush x:Key="plainTextColor2" Color="#606060"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#404040"/>

    <SolidColorBrush x:Key="panelColor" Color="#191F28"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#212831"/>
    <SolidColorBrush x:Key="panelActivateColor" Color="#2A3445"/>
</ResourceDictionary>